{
  "name": "ngd-vis",
  "version": "0.0.1",
  "description": "Understand the learning behavior of natural gradient descent vs SGD and other optimization methods",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ps": "parcel ./src/index.html -d ./public",
    "pb": "parcel build ./src/index.html -d ./public",
    "pw": "parcel watch ./src/index.html -d ./public",
    "stats": "webpack --mode production --json --profile > stats.json",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development --devtool=inline-source-map",
    "watch": "webpack --mode development --devtool=inline-source-map --watch",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --devtool=inline-source-map"
  },
