  },
  "homepage": "https://github.com/bhoov/ngd-vis#readme",
  "dependencies": {
    "@types/d3": "^5.7.2",
    "d3": "^5.11.0",
    "d3-legend": "^1.0.0",
    "d3-selection-multi": "^1.0.1",
    "d3-svg-legend": "^2.25.6",
    "jquery": "^3.4.1",
    "mathjs": "^6.1.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@types/mathjs": "^6.0.1",
